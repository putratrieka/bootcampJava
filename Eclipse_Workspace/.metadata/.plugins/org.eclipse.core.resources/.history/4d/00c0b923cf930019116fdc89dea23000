package com.eksad.training.bean;

public class MotorKopling extends Motor{
	private boolean gigiMasuk ;
	
	public String masukinGigi() {
		gigiMasuk = true;
		
		return "jegreg";
	}
	

	@Override
	public String maju() {
		// TODO Auto-generated method stub
		if (gigiMasuk) {
			return super.maju();
		}else {
			return "..............";
		}
	}
}
